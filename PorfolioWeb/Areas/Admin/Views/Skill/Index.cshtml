@using PorfolioWeb_Models;
@model IEnumerable<Skill>

<table class="table">
	<thead>
		<tr>
			<th>@Html.DisplayNameFor(model=>model.Id)</th>
			<th>@Html.DisplayNameFor(model=>model.Nombre)</th>
			<th>@Html.DisplayNameFor(model=>model.Nivel)</th>
			<th>@Html.DisplayNameFor(model=>model.Descripcion)</th>
			<th>@Html.DisplayNameFor(model=>model.UrlImagen)</th>
		</tr>
	</thead>
	<tbody>
		@foreach(var i in Model)
		{
			<tr>
				<td>@Html.DisplayFor(model=>i.Id)</td>
				<td>@Html.DisplayFor(model=>i.Nombre)</td>
				<td>@Html.DisplayFor(model=>i.Nivel)</td>
				<td>@Html.DisplayFor(model=>i.Descripcion)</td>
				<td>@Html.DisplayFor(model=>i.UrlImagen)</td>
			</tr>
		}
	</tbody>
</table>
<br />
<br />